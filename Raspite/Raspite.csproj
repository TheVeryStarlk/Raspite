<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<IsAotCompatible>true</IsAotCompatible>
		<Version>3.0.0</Version>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Authors>Starlk</Authors>
		<Description>An AOT-friendly NBT serializing tool.</Description>
		<RepositoryUrl>https://github.com/TheVeryStarlk/Raspite</RepositoryUrl>
		<PackageTags>Minecraft, Serializer, NBT, AOT</PackageTags>
		<PackageReleaseNotes>A complete rewrite. Introduces breaking changes.</PackageReleaseNotes>
		<NeutralLanguage>en</NeutralLanguage>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="System.IO.Pipelines" Version="9.0.0"/>
	</ItemGroup>

</Project>